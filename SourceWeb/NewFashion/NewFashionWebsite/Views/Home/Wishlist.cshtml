@{
    ViewBag.Title = "Wishlist";
}
<div style="clear: both; display: block; height: 40px"></div>
<h2>Wishlist &nbsp;<small>Your wishlist shopping</small></h2>
<table class="shopping-cart">
    <tr>
        <th class="image">Image</th>
        <th class="name">Product Name</th>
        <th class="model">Model</th>
        <th class="stock">Stock</th>
        <th class="price">Unit Price</th>
        <th class="action">Action</th>
    </tr>
    <tr>
        <td class="image"><a href="#">
            <img title="product" alt="product" src="@Url.Content("~/Images/products/product-cart-thumb-1.jpg")" height="50" width="50"></a></td>
        <td class="name"><a href="#">T-shirt</a></td>
        <td class="model">Sample Model</td>
        <td class="stock">In Stock</td>
        <td class="price">$48.50</td>
        <td class="remove-update"><a href="#" class="tip remove" title="Remove">
            <img src="@Url.Content("~/Images/remove.png")" alt=""></a>
            <a href="#" class="tip addtocart" title="Add to cart">
                <img src="@Url.Content("~/Images/icon-addcart.png")" alt=""></a></td>
    </tr>
